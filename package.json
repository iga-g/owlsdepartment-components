{
  "name": "owlsdepartment-components",
  "version": "1.0.0",
  "description": "Owls Department components library",
  "main": "index.js",
  "scripts": {
    "dev": "cd playground && vite",
    "serve": "npm run dev",
    "playground": "esr ./scripts/playground.ts",
    "generate": "esr ./scripts/generate.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "yarn eslint && yarn stylelint",
    "lint:no-fix": "yarn eslint:no-fix && yarn stylelint:no-fix",
    "eslint": "eslint src --fix",
    "eslint:no-fix": "eslint src",
    "stylelint": "stylelint src/**/*.{html,css,sass,scss} --fix",
    "stylelint:no-fix": "stylelint src/**/*.{html,css,sass,scss}",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owlsdepartment/owlsdepartment-components.git"
  },
  "keywords": [
    "ui",
    "ovee.js",
    "components"
  ],
  "author": "Owls Department",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/owlsdepartment/owlsdepartment-components/issues"
  },
  "homepage": "https://github.com/owlsdepartment/owlsdepartment-components#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "esbuild": "^0.14.14",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "lodash": "^4.17.21",
    "ovee.js": "~2.1.8",
    "pinst": "^2.0.0",
    "prettier": "^2.4.1",
    "sass": "^1.49.0",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "ovee.js": "~2.1.8"
  },
  "packageManager": "yarn@3.1.1"
}
