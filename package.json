{
  "name": "@ovee.js/toolkit",
  "version": "1.0.0",
  "description": "Owls Department frontend toolkit",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*",
    "./test": {
      "types": "./dist/test.d.ts",
      "import": "./dist/test.mjs",
      "require": "./dist/test.cjs"
    }
  },
  "scripts": {
    "dev": "cd playground && vite",
    "serve": "npm run dev",
    "playground": "esr ./scripts/playground.ts",
    "generate": "esr ./scripts/generate.ts",
    "build": "esr ./scripts/build.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "yarn eslint && yarn stylelint",
    "lint:no-fix": "yarn eslint:no-fix && yarn stylelint:no-fix",
    "eslint": "eslint packages --fix",
    "eslint:no-fix": "eslint packages",
    "stylelint": "stylelint packages/**/*.{html,css,sass,scss} --fix",
    "stylelint:no-fix": "stylelint packages/**/*.{html,css,sass,scss}",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owlsdepartment/owlsdepartment-components.git"
  },
  "keywords": [
    "ui",
    "ovee.js",
    "components"
  ],
  "author": "Owls Department",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/owlsdepartment/owlsdepartment-components/issues"
  },
  "homepage": "https://github.com/owlsdepartment/owlsdepartment-components#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@googlemaps/js-api-loader": "^1.14.3",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/eslint": "^8.4.1",
    "@types/google.maps": "^3.49.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "commander": "^9.0.0",
    "esbuild": "^0.14.14",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gsap": "^3.9.1",
    "husky": "^7.0.0",
    "lodash": "^4.17.21",
    "lottie-web": "^5.8.1",
    "ovee.js": "~2.1.8",
    "pinst": "^2.0.0",
    "prettier": "^2.4.1",
    "rollup": "^2.74.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-typescript-paths": "^1.3.1",
    "sass": "^1.49.0",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "swiper": "^8.0.7",
    "typescript": "^4.5.5",
    "vanilla-lazyload": "^17.5.1",
    "vite": "^2.7.13"
  },
  "peerDependencies": {
    "@googlemaps/js-api-loader": "^1.14.3",
    "lodash": "^4.17.21",
    "lottie-web": "^5.8.1",
    "ovee.js": "~2.1.8",
    "swiper": "^8.0.7",
    "vanilla-lazyload": "^17.5.1"
  },
  "packageManager": "yarn@3.1.1"
}
